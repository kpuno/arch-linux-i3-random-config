{
   "background": {
      "scripts": [ "event.js" ]
   },
   "browser_action": {
      "default_icon": "icon128.png",
      "default_popup": "popup.html",
      "default_title": "Disable"
   },
   "description": "Allow the browser to play HLS video urls (m3u8) 'natively'",
   "icons": {
      "128": "icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Native HLS Playback",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "tabs", "*://*/*.m3u8*", "webRequest", "webRequestBlocking", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.5.52",
   "web_accessible_resources": [ "hls.0.5.51.min.js", "hls.0.5.52.min.js", "hls.0.6.8.min.js", "hls.0.6.9.min.js", "*.html" ]
}
